          </if>
          <if test="updateUserName != null">
          	update_user_name,
          </if>
          <if test="updateTime != null">
          	update_time,
          </if>
        </trim>
        <trim prefix="VALUES (" suffix=")" suffixOverrides=",">            
          <if test="permissionName != null" >
