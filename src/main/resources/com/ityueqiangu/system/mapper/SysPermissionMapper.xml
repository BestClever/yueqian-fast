          <if test="updateUserId != null" >
            #{updateUserId},                    
          </if>
          <if test="updateUserName != null" >
            #{updateUserName},                    
          </if>
          <if test="updateTime != null" >
            #{updateTime},                    
          </if>
        </trim>
    </insert>    
    
    <!-- 更新不为NULL的字段 -->
    <update id="updateSysPermission" parameterType="com.ityueqiangu.system.domain.SysPermission">
    UPDATE sys_permission
