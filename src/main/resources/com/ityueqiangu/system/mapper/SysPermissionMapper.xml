          	permission_name,
          </if>
          <if test="permissionType != null">
          	permission_type,
          </if>
          <if test="permissionCode != null">
          	permission_code,
          </if>
          <if test="permissionUrl != null">
          	permission_url,
          </if>
          <if test="openType != null">
          	open_type,
          </if>
          <if test="parentId != null">
          	parent_id,
          </if>
          <if test="icon != null">
          	icon,
          </if>
          <if test="sortNum != null">
          	sort_num,
          </if>
          <if test="isSpread != null">
